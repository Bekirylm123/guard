datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Guild {
    id     String @id
    prefix String @default("$")
}

model Member {
    id       String @id
    guildId  String
    warnings Int    @default(0)

    @@unique([id, guildId])
}
